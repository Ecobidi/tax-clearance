<div class="row">
  <div class="col-12">
    <h4>Your Account Information</h4>
    <div>
      <table class="table table-dark table-striped my-4 mx-4">
        <tr>
          <td>Account No.</td>
          <td><% account.account_no %></td>
        </tr>
        <tr>
          <td>Account Type</td>
          <td><% account.account_type %></td>
        </tr>
        <tr>
          <td>Account Owner.</td>
          <td><% account.customer_name %></td>
        </tr>
        <tr>
          <td>Contact Account Owner.</td>
          <td><% account.customer.phone %></td>
        </tr>
        <tr>
          <td>Account Created.</td>
          <td><% account.created_at %></td>
        </tr>
      </table>
    </div>
  </div>
</div>